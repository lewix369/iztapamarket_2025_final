{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "^/api/create_preference_v2$",
      "dest": "/api/create_preference_v2.mjs"
    },
    { "src": "^/api/create_preference$", "dest": "/api/create_preference" },
    { "src": "^/api/webhook_mp$", "dest": "/api/webhook_mp.mjs" },
    { "src": "^/assets/(.*)$", "dest": "/assets/$1" },
    {
      "src": "^/(robots\\.txt|sitemap\\.xml|favicon\\.ico|apple-touch-icon\\.png|site\\.webmanifest|manifest\\.webmanifest)$",
      "dest": "/$1"
    },
    {
      "src": "^/service-worker\\.js$",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" },
      "dest": "/service-worker.js"
    },
    {
      "src": "^/(workbox-.*\\.js)$",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" },
      "dest": "/$1"
    },
    {
      "src": "^/(images|img|icons|logos|static|media)/(.*)$",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1/$2"
    },
    {
      "src": "^/(.*\\.(?:png|jpg|jpeg|gif|svg|webp|ico|json|txt|xml|css))$",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/$1"
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
