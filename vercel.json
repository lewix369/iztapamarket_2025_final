{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/google3a072cd80aefa66d.html",
      "destination": "/google3a072cd80aefa66d.html"
    },
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/api/sitemap", "destination": "/api/sitemap" },
    {
      "source": "/manifest.webmanifest",
      "destination": "/manifest.webmanifest"
    },
    { "source": "/service-worker.js", "destination": "/service-worker.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "redirects": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "x-vercel-skip-feedback-widget", "value": "1" }]
    },
    {
      "source": "/google3a072cd80aefa66d.html",
      "headers": [
        { "key": "x-vercel-skip-feedback-widget", "value": "1" },
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    },
    {
      "source": "/index.html",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    },
    {
      "source": "/:path*\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|woff2|ttf|json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/service-worker.js",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    },
    {
      "source": "/api/sitemap",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" },
        { "key": "x-vercel-skip-feedback-widget", "value": "1" }
      ]
    }
  ]
}
