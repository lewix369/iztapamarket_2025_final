{
  "name": "iztapamarket",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev:frontend": "vite --port 5173 --strictPort --mode sandbox",
    "dev:backend:local": "DOTENV_CONFIG_PATH=.env.local node server.mjs",
    "dev:backend": "DOTENV_CONFIG_PATH=.env.production node server.mjs",
    "dev": "concurrently -n FRONT,BACK -c green,blue \"npm run dev:frontend\" \"DOTENV_CONFIG_PATH=.env.sandbox node server.mjs\"",
    "build": "vite build",
    "preview": "vite preview",
    "backend": "node server.mjs",
    "start:backend": "DOTENV_CONFIG_PATH=.env.production node server.mjs",
    "start": "npm run dev",
    "dev:clean": "pkill -f 'vercel dev' || true && pkill -f vite || true && npx vercel dev --listen=3003",
    "gen:sitemap:sandbox": "dotenv -e .env.sandbox -- node scripts/generate-sitemap.mjs",
    "gen:sitemap:prod": "dotenv -e .env.production -- node scripts/generate-sitemap.mjs"
  },
  "dependencies": {
    "@mercadopago/sdk-react": "^1.0.6",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "express": "^5.1.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.285.0",
    "mercadopago": "^1.5.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.16.0",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@eslint/js": "^9.37.0",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vercel/node": "^5.3.21",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "terser": "^5.39.0",
    "vite": "^4.5.14"
  },
  "proxy": "http://localhost:3000"
}
